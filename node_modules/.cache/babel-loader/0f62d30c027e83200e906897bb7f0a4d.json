/li>\n</template>\n\n<script>\nexport default {\n  name: \"CardLanguage\",\n  props: {\n    title: {\n      type: String,\n      default: \"\",\n    },\n    imgUrl: {\n      type: String,\n      default: \"\",\n    },\n    wikipedia: {\n      type: String,\n      default: \"\",\n    },\n    description: {\n      type: String,\n      default: \"\",\n    },\n  },\n  created() {\n    // console.log(this.imgUrl)\n  },\n};\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nli {\n  list-style: none;\n  margin: 1vw;\n}\n.thecard {\n  position: relative;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  border-radius: 10px;\n  overflow: hidden;\n}\n.thefront {\n  position: absolute;\n  top: 0;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  border-radius: 10px;\n  backface-visibility: hidden;\n  overflow: hidden;\n  /* background-color: #29ABE2; */\n  background-color: #ffab1a;\n  color: white;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: flex-end;\n}\n.theback {\n  position: absolute;\n  top: 100%;\n  left: 0;\n  width: 100%;\n  height: 100%;\n  border-radius: 10px;\n  overflow: hidden;\n  /* background: #1885b4; */\n  background-color: #ff5f1a;\n  color: #fff;\n  text-align: center;\n}\n.thecard:hover > .theback {\n  animation-name: hoverCard;\n  animation-timing-function: ease;\n  animation-duration: 1s;\n}\n.theback:hover {\n  top: 0%;\n}\n.theback a {\n  color: white;\n}\n.thefront h1 {\n  font-family: \"Helvetica\", sans-serif;\n  font-weight: bold;\n  font-size: 20px;\n  margin: 10px 0;\n  text-align: center;\n}\n.theback p {\n  font-family: \"Helvetica\", sans-serif;\n  padding: 15px;\n  font-weight: normal;\n  font-size: 13px;\n}\n@keyframes hoverCard {\n  0% {\n    top: 100%;\n  }\n\n  100% {\n    top: 0%;\n  }\n}\n\n@media (max-width: 680px) {\n  .cardcontainer {\n    width: 90px;\n    height: 130px;\n    background: none;\n  }\n  .img-card {\n    height: 70px;\